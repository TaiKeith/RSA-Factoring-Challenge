import sys
import sympy


def factorize(n):
    factors = sympy.factorint(n)
    return factors


def main(filename):
    try:
        with open(filename, "r") as f:
            n = int(f.read().strip())

        factors = factorize(n)
        p, q = factors.keys()
        print(f"{n}={p}*{q}")

    except FileNotFoundError:
        print(f"File '{filename}' not found")
    except ValueError:
        print("Invalid input in file. Make sure it contains a natural number")


if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: rsa <file>")
        sys.exit()

    main(sys.argv[1])
